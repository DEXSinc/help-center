{
  "entrypoint": "index.html",
  "shell": "src/app.html",
  "fragments": [
    "src/views/home.html",
    "src/views/articles.html",
    "src/views/faqs.html",
    "src/views/view404.html",
    "src/views/videos.html"
  ],
  "sources": [
    "src/**/*",
    "assets/**/*",
    "images/**/*",
    "data/**/*",
    "elements/address-form.html",
    "elements/dashboard-stats-card.html",
    "node_modules/redux/dist/redux.js",
    "node_modules/redux-thunk/dist/redux-thunk.js",
    "lib/lodash/lodash.js",
    "lib/webcomponentsjs/webcomponents-lite.min.js",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2-hybrid"],
    "ignoreWarnings": [
      "multiple-doc-comments",
      "unknown-polymer-behavior",
      "overriding-private"
    ]
  },
  "builds": [
    { "preset": "es5-bundled" },
    { "preset": "es6-unbundled" }
  ]
}
